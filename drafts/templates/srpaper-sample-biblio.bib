@article{Kononenko01machinelearning,
  author = "Igor Kononenko",
  title = "Machine learning for medical diagnosis: history, state of the art and perspective",
  journal = "Artificial Intelligence in Medicine",
  volume = 23,
  pages = "89--109",
  year = 2001,
  annotate = "Describes the history of machine learning for medical diagnosis with an emphasis on Naive Bayes, neural networks, and decision trees. Emphasizes the need for ML algorithms in this domain to be able to handle noisy and missing data, rely on relatively few medical tests, and complement the role of physicians. Seven ML algorithms are evaluated on several criteria in general, as well as when applied to ischemic heart disease diagnosis."
}

@article{kukar:analysing,
  author = "Matjaz Kukar and Igor Kononenko and Ciril Groselj and Katarina Kralj and Jure Fettich",
  title = "Analysing and improving the diagnosis of ischaemic heart disease with machine learning",
  journal = "Artificial Intelligence in Medicine",
  pages = "25--50",
  year = 1999,
  annotate = "Collected data on ischemic heart disease consisting of signs and symptoms, ECG, and scintigraphy. Applied Naive Bayes, neural networks, k-nearest neighbors, and two decision tree algorithms, and compared these to clinicians' diagnoses. Naive Bayes had the best sensitivity (recall), whereas clinicians, followed by neural nets, had the highest specificity."
}

@inproceedings{Güvenir98asupervised,
  author = "H. Altay Güvenir and Burak Acar and Gülsen Demiröz and Ayhan Çekin",
  title = "A Supervised Machine Learning Algorithm for Arrhythmia Analysis",
  booktitle = "In: Proc. Computers in Cardiology Conference",
  pages = "433--436",
  year = 1998,
  annotate = "Presents a novel ML approach to diagnosing and classifying cardiac arrhythmia, called the VF15 algorithm. It uses a genetic algorithm to learn feature weights. Then, each feature 'votes' on a class prediction. The algorithm has a 62\% accuracy on this particular task and is found to outperform Naive Bayes."
}

@inproceedings{DemirozG97,
  author = "Gülsen Demiröz and H. Altay Güvenir",
  title = "Classification by Voting Feature Intervals",
  booktitle = "Machine Learning: ECML-97, 9th European Conference on Machine Learning, Prague, Czech Republic, April 23-25, 1997, Proceedings",
  pages = "85--92",
  publisher = "Springer",
  year = 1997,
  annotate = "Provides a description of the VFI (Voting Feature Intervals) algorithm, on which VF15 is based."
}

@inproceedings{Gao05bayesianann,
  author = "Dayong Gao and Michael Madden and Des Chambers and Gerard Lyons",
  title = "Bayesian ANN Classifier for ECG Arrhythmia Diagnostic System: A Comparison Study",
  booktitle = "Proc. of Int. Joint Conf. on Neural Networks",
  year = 2005,
  annotate = "Compares various ML algorithms for arrhythmia diagnosis based on ECG data, with an emphasis on minimizing false positives and dealing with noisy data. Highlights need to improve on VF15's \%62 accuracy. Uses the UCI ML Repository Arrhythmia dataset. Evaluates a Bayesian artifical neural network classifier as compared to Naive Bayes, decision trees, logistic regression, and neural networks."
}


@article {detrano1989international,
  author = "R Detrano",
  title = "International Application of a New Probability Algorithm for the Diagnosis of Coronary Artery Disease",
  journal = "American Journal of Cardiology",
  pages = "304--310",
  volume = "64",
  year = "1989",
  annotate = "Derives a model from the Cleveland Clinic heart disease data set (also available at UCI ML Repository) and compares it to the CADENZA Bayesian algorithm. Both models are found to overpredict heart disease, though this occurs more with CADENZA."
}

@inproceedings{das2012,
  author = "Samarjit Das and Breogan Amoedo and F. De la Torre and Jessica Hodgins",
  title = "Detecting Parkinsons' symptoms in uncontrolled home environments: A multiple instance learning approach",
  booktitle = "Annual International Conference of the IEEE",
  pages = "3688--3691",
  year = 2012,
  annotate = "Describes a weakly supervised multiple instance learning approach to detecting symptoms of Parkinson's Disease. This approach addresses the issue of self-reporting resulting inaccurate or incomplete labels."
}

@article{tsanas2010,
  author = "Athanasios Tsanas and Max A. Little and Patrick E. McSharry and Lorraine O. Ramig",
  title = "Accurate Telemonitoring of Parkinson’s Disease Progression by Noninvasive Speech Tests",
  journal = "IEEE Transactions on Biomedical Engineering",
  volume = 57,
  number = "4",
  pages = "884--893",
  year = 2010,
  annotate = "Proposes monitoring Parkinson's disease using speech tests, due to vocal impairment being a common symptom and early indicator. The test results are passed through signal processing algorithms and a classification and regression tree to predict a rating on the unified PD rating scale."
